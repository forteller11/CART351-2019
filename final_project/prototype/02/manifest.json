
{
  "manifest_version": 2,

  "name": "project prototype",
  "author": "Charles Yan Miller",
  "description": "prototype",
  "version": "1.0",

  "icons": {  "16" : "icon.png",
              "48" : "icon.png",
              "128" : "icon.png"
            },

  "content_scripts": [
    {
      "js": [ "p5.js","p5.dom.js","Content/main.js","Content/Tools/Tool.js","Content/Tools/Brush.js","jquery-3.1.1.min.js"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
    }
  ],

  "background": {
    "scripts": []
  },

  "browser_action": {
    "default_title": "Charly Yan Miller -- CART 351 Project Prototype.",
    "default_icon": {
      "19": "icon_19.png"
    },
    "default_popup": "popup.html",
    "default_title": "Begin Painting?"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],

"web_accessible_resources": []

}
